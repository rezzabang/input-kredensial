generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Dataribadi {
  nip          String   @id
  nama         String
  tempatLahir  String
  tanggalLahir DateTime
  jenisKelamin String
  phone        String
  email        String
  alamat       String
}
